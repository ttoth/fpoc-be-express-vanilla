{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "space-before-function-paren": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      4
    ],
    "max-len": [
      "error",
      {
        "code": 120
      }
    ]
  }
}